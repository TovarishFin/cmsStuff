extends layout
block content
	h1 register
	form(method="post")
		span username
		input(type="text",name="username")
		br
		span password
		input(type="password",name="password")
		br
		span email
		input(type="text",name="email")
		br
		span type
		select
			option(value="admin",name="admin") admin
			option(value="cs",name="cs") cs
			option(value="qa",name="qa") qa
			option(value="ret",name="ret") ret
		br
		input(type="submit",value="register")
